<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        background-color: #f2f2f2;
        padding: 20px;
      }

      h1 {
        margin-top: 0;
      }

      table {
        width: 100%;
        border-collapse: collapse;
        background-color: #fff;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        margin-bottom: 20px;
      }

      table td,
      table th {
        padding: 10px;
        text-align: left;
      }

      table th {
        background-color: #4caf50;
        color: #fff;
      }

      table tr:nth-child(even) {
        background-color: #f2f2f2;
      }
    </style>
  </head>
  <body>
    <h1>Detail Data</h1>
    <table>
      <tr>
        <td>ID</td>
        <td>:</td>
        <td><%= data.id %></td>
      </tr>
      <tr>
        <td>Username</td>
        <td>:</td>
        <td><%= data.username %></td>
      </tr>
      <tr>
        <td>Password</td>
        <td>:</td>
        <td><%= data.password %></td>
      </tr>
      <tr>
        <td>Created At</td>
        <td>:</td>
        <td><%= data.createdAt %></td>
      </tr>
      <tr>
        <td>Updated At</td>
        <td>:</td>
        <td><%= data.updatedAt %></td>
      </tr>
    </table>

    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Score</th>
          <th>CreatedAt</th>
          <th>UpdatedAt</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i < data.UserScores.length; i++) { %>
        <tr>
          <td><%= data.UserScores[i].id %></td>
          <td><%= data.UserScores[i].score %></td>
          <td><%= data.UserScores[i].updatedAt %></td>
          <td><%= data.UserScores[i].deletedAt %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
